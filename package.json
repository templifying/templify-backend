    "esbuild": "^0.25.5",
    "puppeteer-core": "^24.11.1",
    "serverless": "^3.40.0",
    "serverless-domain-manager": "^8.0.0",
    "serverless-esbuild": "^1.55.1",
    "serverless-offline": "^13.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
