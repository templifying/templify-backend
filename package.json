{
  "name": "mkpdfs-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "lint": "echo \"No linter configured\"",
    "deploy:dev": "AWS_PROFILE=rocketeast serverless deploy --stage dev",
    "deploy:stage": "AWS_PROFILE=rocketeast serverless deploy --stage stage",
    "deploy:prod": "AWS_PROFILE=rocketeast serverless deploy --stage prod",
    "offline": "AWS_PROFILE=rocketeast IS_OFFLINE=true serverless offline --stage dev",
    "remove:dev": "AWS_PROFILE=rocketeast serverless remove --stage dev",
    "remove:stage": "AWS_PROFILE=rocketeast serverless remove --stage stage",
    "remove:prod": "AWS_PROFILE=rocketeast serverless remove --stage prod",
    "info:dev": "AWS_PROFILE=rocketeast serverless info --stage dev",
    "info:stage": "AWS_PROFILE=rocketeast serverless info --stage stage",
    "info:prod": "AWS_PROFILE=rocketeast serverless info --stage prod",
    "logs": "serverless logs -f $FUNCTION --stage dev"
  },
